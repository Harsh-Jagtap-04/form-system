{"ast":null,"code":"import MultiStepForm from \"@/components/MultiStepForm.vue\";\nimport SchemeTable from \"@/components/SchemeTable.vue\";\nimport ApplicationPage from \"@/components/ApplicationPage.vue\";\n// ... (import other components)\n\nexport default {\n  name: 'DashboardPage',\n  components: {\n    MultiStepForm,\n    SchemeTable,\n    ApplicationPage\n    // ... (other components)\n  },\n\n  data() {\n    return {\n      selectedOption: null\n    };\n  },\n  computed: {\n    selectedOptionComponent() {\n      return this.selectedOption === 'apply_scheme' ? 'MultiStepForm' : this.selectedOption === 'schemes' ? 'SchemeTable' : this.selectedOption === 'applications' ? 'ApplicationPage' : null;\n    }\n  },\n  methods: {\n    selectOption(option) {\n      this.selectedOption = option;\n    }\n  }\n};","map":{"version":3,"names":["MultiStepForm","SchemeTable","ApplicationPage","name","components","data","selectedOption","computed","selectedOptionComponent","methods","selectOption","option"],"sources":["C:\\GovProject\\form-system\\src\\components\\DashboardPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <div v-if=\"selectedOption === null\">\r\n      <div class=\"dashboard-block\" @click=\"selectOption('apply_scheme')\">\r\n        <div class=\"block-content\">\r\n          <h2>Apply for Scheme</h2>\r\n        </div>\r\n      </div>\r\n      <div class=\"dashboard-block\" @click=\"selectOption('schemes')\">\r\n        <div class=\"block-content\">\r\n          <h2>All Schemes</h2>\r\n        </div>\r\n      </div>\r\n      <div class=\"dashboard-block\" @click=\"selectOption('applications')\">\r\n        <div class=\"block-content\">\r\n          <h2>Applications</h2>\r\n        </div>\r\n      </div>\r\n      <!-- Add more blocks for other options -->\r\n    </div>\r\n    <div v-else>\r\n      <component :is=\"selectedOptionComponent\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MultiStepForm from \"@/components/MultiStepForm.vue\";\r\nimport SchemeTable from \"@/components/SchemeTable.vue\";\r\nimport ApplicationPage from \"@/components/ApplicationPage.vue\";\r\n// ... (import other components)\r\n\r\nexport default {\r\n  name: 'DashboardPage',\r\n  components: {\r\n    MultiStepForm,\r\n    SchemeTable,\r\n    ApplicationPage,\r\n    // ... (other components)\r\n  },\r\n  data() {\r\n    return {\r\n      selectedOption: null\r\n    };\r\n  },\r\n  computed: {\r\n    selectedOptionComponent() {\r\n      return this.selectedOption === 'apply_scheme'\r\n        ? 'MultiStepForm'\r\n        : this.selectedOption === 'schemes'\r\n        ? 'SchemeTable'\r\n        : this.selectedOption === 'applications'\r\n        ? 'ApplicationPage'\r\n        : null;\r\n    }\r\n  },\r\n  methods: {\r\n    selectOption(option) {\r\n      this.selectedOption = option;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* This is the scope of the DashboardPage component */\r\n.dashboard {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.dashboard-block {\r\n  width: calc(33.33% - 20px);\r\n  padding: 20px;\r\n  background-color: #f1f1f1;\r\n  box-shadow: 0 0 2px var(--grey-color-light);\r\n  transition: background-color 0.3s, transform 0.3s;\r\n  cursor: pointer;\r\n  border-radius: 8px; /* Add border radius for a softer look */\r\n}\r\n\r\n.dashboard-block:hover {\r\n  background-color: #4361ee;\r\n  color: white;\r\n  transform: translateY(-5px);\r\n}\r\n\r\n.block-content {\r\n  text-align: center;\r\n}\r\n\r\n.block-content h2 {\r\n  margin-bottom: 10px;\r\n  font-size: 1.2rem;\r\n  color: inherit; /* Inherit text color from the parent element */\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,eAAc,MAAO,kCAAkC;AAC9D;;AAEA,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVJ,aAAa;IACbC,WAAW;IACXC;IACA;EACF,CAAC;;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE